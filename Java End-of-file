import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        try{
        int i=1;
        while(sc!=null){
            String txt= sc.nextLine();
            System.out.print(i+" ");
            System.out.println(txt);
            i++;
        }
        }
        catch(Exception e){
            //System.out.println(e);
        }
    }
}
